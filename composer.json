{
    "name": "jletrondo/upload",
    "description": "This is a minimalistic, extensible PHP uploading library with a structure inspired by the CodeIgniter Upload Library but streamlined and modern.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Jletrondo\\Upload\\": "src/"
        }
    },
    "authors": [
        {
            "name": "jesrel",
            "email": "jletrondo@geniihut.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },"autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "@php vendor/bin/pest"
    }
}